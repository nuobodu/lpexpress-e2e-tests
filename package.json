{
  "name": "lpexpress-e2e-tests",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cy:open": "npx cypress open",
    "cy:r:ch": "npx cypress run --browser chrome",
    "clean:reports": "rimraf cypress/reports",
    "merge:reports": "mochawesome-merge cypress/reports/json/*.json > cypress/reports/merged-report.json",
    "generate:html": "marge cypress/reports/merged-report.json -f index -o cypress/reports/html --inline",
    "report:ci": "npm run merge:reports && npm run generate:html",
    "cy:r:report:local": "npm run clean:reports && npm run cy:r:ch && npm run report:ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuobodu/lpexpress-e2e-tests.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nuobodu/lpexpress-e2e-tests/issues"
  },
  "homepage": "https://github.com/nuobodu/lpexpress-e2e-tests#readme",
  "devDependencies": {
    "cypress": "^15.4.0",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.3.0",
    "rimraf": "^6.0.1"
  }
}
